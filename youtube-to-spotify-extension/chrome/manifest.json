{
    "name": "Youtube to Spotify",
    "version": "1.0.0",
    "description": "This is an extension to automatically save a Youtube music to your Spotify",
    "manifest_version": 3,
    "background": {
        "scripts": [
        "scripts/oauth.js",
        "scripts/api_calls.js",
        "scripts/utils.js",
        "scripts/events_listeners.js"
        ]
    },
    "permissions": [
        "storage",
        "scripting",
        "activeTab",
        "tabs",
        "webRequest",
        "webRequestBlocking"
    ],
    "icons": {
        "48": "imgs/icon.png"
      },

    "author": "Tiago Ribeiro",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Youtube To Spotify"
    }
}